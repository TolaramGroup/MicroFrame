{
    "name": "gpproton/microframe",
    "description": "A simplistic micro-plain framework with a little touch of MVC without the pains of models",
    "keywords": ["template", "framework", "MVC", "micro", "cli"],
    "homepage": "https://github.com/gpproton/microframe",
    "type": "template",
    "license": "MIT",
    "authors": [
        {
            "name": "Godwin peter .O",
            "email": "me@godwin.dev",
            "homepage": "https://godwin.dev",
            "role": "Developer"
        }
    ],
    "config": {
        "platform": {
        }
    },
    "autoload": {
        "psr-4": {
            "MicroFrame\\": "MicroFrame",
            "App\\": "App"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "predis/predis": "1.1.*",
        "taq/pdooci": "1.0.*",
        "phpfastcache/phpfastcache": "^8.0",
        "hassankhan/config": "2.1.*",
        "symfony/yaml": "5.1.*",
        "symfony/finder": "5.1.*",
        "symfony/serializer": "3.4.43",
        "cebe/markdown": "1.2.*",
        "zircote/swagger-php": "3.0.*",
        "asyncphp/doorman": "3.1.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "filp/whoops": "2.7.*",
        "phpunit/phpunit": "9.2.*"
    },
    "scripts": { }
}
