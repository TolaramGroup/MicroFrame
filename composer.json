{
    "name": "gpproton/microframe",
    "description": "A simplistic micro framework with a little touch of MVC without the necessity of models",
    "keywords": ["template", "framework", "MVC", "micro", "cli"],
    "homepage": "https://github.com/gpproton/microframe",
    "type": "template",
    "license": "MIT",
    "authors": [
        {
            "name": "Godwin peter .O",
            "email": "me@godwin.dev",
            "homepage": "https://godwin.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/gpproton/microframe/issues",
        "source": "https://github.com/gpproton/microframe"
    },
    "config": {
        "platform": { },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MicroFrame\\": "MicroFrame",
            "App\\": "App"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "MicroFrame/Tests/",
            "App\\Tests\\": "App/Tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "predis/predis": "1.1.*",
        "taq/pdooci": "1.0.*",
        "phpfastcache/phpfastcache": "^8.0",
        "hassankhan/config": "2.1.*",
        "symfony/yaml": "5.1.*",
        "symfony/finder": "5.1.*",
        "symfony/serializer": "3.4.43",
        "cebe/markdown": "1.2.*",
        "zircote/swagger-php": "3.0.*",
        "asyncphp/doorman": "3.1.*"
    },
    "require-dev": {
        "filp/whoops": "2.7.*",
        "phpunit/phpunit": "7.5.*",
        "friendsofphp/php-cs-fixer": "2.16.*",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "suggest": {
        "ext-ftp": "Required to use the Flysystem FTP driver.",
        "ext-gd": "Required to use for...",
        "ext-memcached": "Required to use the memcache cache driver.",
        "ext-pcntl": "Required to use all features of the queue worker.",
        "ext-posix": "Required to use all features of the queue worker.",
        "ext-redis": "Required to use the Redis cache and queue drivers (^4.0|^5.0).",
        "mockery/mockery": "Required to use mocking (^1.3.1)."
    },
    "scripts": {
        "post-install-cmd": [

        ],
        "post-update-cmd": [

        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
